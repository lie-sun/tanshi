.page-body-button {
  margin-bottom: 30rpx;
}
.page-body{
  width: 90%;
  margin-left: 5%;
  color: #000;
}
.page-header-wrap{
  margin-bottom: 12px;
}
live-player {
  width: 100%;
  height: 230px;
}
image{
  width: 100%;
}
.button{
  height: 40px;
  line-height: 40px;
  color: #fff;
  background: #f568a9;
 font-size: 32rpx;
}
.page-header-rignt-wrap {
  float: right; 
}
.custom-select-btn-area {
  position: relative;
}
.custom-select-wrap{
  display: inline-block;
}
.custom-select-wrap .custom-select-btn-area {
  width: 140rpx;
  margin: 0;
}
button.custom-select-btn {
  height: 65rpx;
  line-height: 65rpx;
  font-size: 30rpx;
  color: #000;
  text-align: left;
  border-radius: 20px;
  border: 1px solid #f0f0f0;
  box-shadow: none;
  font-family: SimSun;
}
.triangle-down{
  position: absolute;
  top: 28rpx;
  right: 20rpx;
  width: 0;
  height: 0;
  border-left: 6px solid transparent;
  border-right: 6px solid transparent;
  border-top: 7px solid #000;
}
.page-header-right-text {
  position: absolute;
  top: 4rpx;
  left: -110rpx;
  display: inline-block;
  font-size: 32rpx;
  color: #999;
}
.page-header-rignt-position-wrap {
  position: relative;
}
.page-header-left {
  font-size: 32rpx;
  font-family: SimSun;
  font-weight: bold;
}

.player-weui-cells-wrap::before {
  border-top: none;
}
.player-weui-cell::before {
  border-top: none;
}
.player-checkbox-wrap {
  text-align: center;
  margin:20px 0 10px 0;
}
.player-img {
  margin-top: 10px;
  width: 100%;
  height: 320rpx;
}






.play-no-free{
  position: relative;
  width: 100%;

}

.play-no-free-text-wrap {
  width: 100%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform:translate(-50%,-50%);
}

.play-no-free-wrap image{
  height: 230px;
  width: 100%;
}
.play-no-free-text{
  color: #fff;
  display: block;
  text-align: center;
  font-size: 32rpx;
}
.play-no-free-title-text {
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  display: block;
  margin-top: 30rpx;
}
.play-no-free-btn {
  width: 30%;
  height: 50rpx;
  line-height: 45rpx;
  background: #f568a9;
  color: #fff;
  font-size: 28rpx;
  margin-top: 30rpx;
}

.play-no-vedio-text {
  font-size: 30rpx;
}
.play-no-vedio-btn {
   width: 30%;
  height: 50rpx;
  line-height: 45rpx;
  background: #999;
  color: #fff;
  font-size: 28rpx;
  margin-top: 30rpx;
}


.no-statement{
  width: 100%;
  height: 230px;
  background: #000;
  line-height: 230px;
  font-size: 28rpx;
}

.rich-wrap{
  font-size: 28rpx;
  color: #666;
}

.self-video-wrap{
  position: relative;
}

.weui-loadmore-wrap{
  position: absolute;
  top: 40%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 9999;
}
.weui-loadmore__tips{
  color: #fff;
}
.logo-img{
  position: absolute;
  top:20px;
  left:4px;
  z-index: 9999;
}
.logo-img image {
  width: 100rpx;
  height: 60rpx;
}


.unlog-text-wrap{
  margin-top:40%;
text-align: center;
}
.unlog-text{
  font-size: 30rpx;
  
}
.index-panel-header {
  height: 90px;
  background: #f568a9;
  display: flex;
  justify-content: center;
  align-items:center;
}
.index-click-wrap{
  color: #fff;
  text-align: center;
  font-size: 30rpx;
  width: 280rpx;
  padding: 6px 0;
  border: 1px solid #eb5c9e;
  background: #f17cb3;
  border-radius: 4px;

}

.no-statement text{
  color: #fff;
  font-family: SimSun;
  font-size: 28rpx;
  display:block;
text-align:center;
}
.weui-agree__text{
  font-size: 28rpx;
  transform: scale(1.3);
}
.authorize-btn-wrap{
  position: relative;
  height: 40px;
}
.authorize-btn{
  position: absolute;
  width: 200px;
  height: 40px;
  line-height: 40px;
  left: 50%;
  margin-left: -100px;
  top: 10px;
}
.rich-view-wrap{
  margin: 0 5%;
}
